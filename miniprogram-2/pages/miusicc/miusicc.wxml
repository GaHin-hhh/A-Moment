        <view>
          <view class='zhubo-name'>梦呓</view>
          <view class='zhubo-from'>2019/09/09</view>
        </view>
      </view>
  </view>
<view class='zhubo1'> 
      <view class='zhuboLeft1'>
        <view class='zhubo-avater1'>
          <image class='zhuboIcon1' src="/image/bar/1206803589.jpg"></image>
