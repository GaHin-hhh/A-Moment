.clear{
  clear: both;
  overflow: hidden;
}
navigator{
  display:inline;
}
.list{
  margin-top:10px;
}
 
.list .list_item{
  padding-top:10px;
  padding-bottom:100px;
  padding-left:10px;
  height:100px;
  border-bottom:1px solid #E8E8E8;
}
.list .list_item .img{
  float:left;
  width:50%;
  height:90%;
}
.list .list_item .img image{
  width:90%;
  height:210%;

}
 
.list .list_item .info{
  width:50%;
  float:right;
  height:90px;
  position:relative;
  padding-top:30px;
}
.list .list_item .info .title{
  color:#333;
  margin-left:10px;
  font-size: 18px;
}
 
.list .list_item .info .price{
  color:rgb(0, 0, 0);
  margin-left:10px;
  margin-top:10px;
  font-size:15px;
}
 
.list .list_item .info .num{
  position: absolute;
  left:0px;
  bottom:5px;
  color:#747474;
  margin-left:10px;
  font-size:15px;
}
.list .list_item .info .juli{
padding-top:60px;
}
.button-green {
  background-color: #949090; /* Green */
}
.button-red {
  background-color: #9c9c9c; /* 红色 */
}

button {
  color: rgb(255, 255, 255);
  text-decoration: none;
  display: inline-block;
  font-size: 30rpx;
  border-radius: 0rpx;
  width: 50%;
  height: 50%;
  line-height: 50rpx;
}
